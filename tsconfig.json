{
  "compilerOptions": {
    "target": "es2018",
    "module": "esnext",
    "moduleResolution": "node",
    "lib": [
      "esnext",
      "esnext.asynciterable",
      "dom"
    ],
    "esModuleInterop": true,
    "allowJs": true,
    "sourceMap": true,
    "strict": true,
    "noEmit": true,
    "baseUrl": ".",
    "paths": {
      "~/*": [
        "./*"
      ],
      "@/*": [
        "./*"
      ]
    },
    "types": [
      "@types/node",
      "@nuxt/types",
      "vue-router",
      "nuxt-i18n",
      "vue-js-modal/types",
      "vue-apollo/types",
      "./types"
    ],
    "resolveJsonModule": true,
    "noImplicitAny": false,
    "experimentalDecorators": true
  },
  "exclude": [
    "node_modules"
  ]
}
