{
  "name": "project-to-share",
  "version": "1.0.0",
  "description": "learning by homepage on nuxt.js",
  "author": "Yelissey Balanov",
  "private": true,
  "scripts": {
    "precommit": "npm run lint",
    "dev": "cross-env NUXT_ENV_IGNORE_WS=true yarn dev-with-ws",
    "dev-with-ws": "cross-env SENTRY_DISABLED=true nuxt-ts dev --hostname 0.0.0.0 --port 9762",
    "build": "cross-env SENTRY_DISABLED=true nuxt-ts build",
    "start": "cross-env SENTRY_DISABLED=true nuxt-ts start --hostname localhost --port 9762",
    "build-prod": "nuxt-ts build && rimraf .nuxt/dist/**/*.js.map",
    "start-prod": "nuxt-ts start --hostname localhost --port 9762",
    "start-staging": "nuxt-ts start --hostname localhost --port 9761",
    "generate": "nuxt-ts generate",
    "tsc": "tsc --showConfig",
    "lint": "tslint --project .",
    "lintfix": "prettier --write \"{assets,components,helpers,layouts,middleware,pages,plugins,store}/**/*.{ts,vue,scss}\" && tslint --fix --project .",
    "graphql-codegen": "graphql-codegen --config codegen.yml",
    "start-http-dist": "ws -d dist -s index.html -p 4500",
    "fetch-image-sizes": "node fetchImageSizes.js"
  },
  "dependencies": {
    "@nuxt/postcss8": "^1.1.3",
    "@nuxt/typescript-runtime": "^2.0.1",
    "@nuxtjs/pwa": "^3.0.0-beta.20",
    "@nuxtjs/sentry": "^4.1.2",
    "@popperjs/core": "^2.6.0",
    "@types/node": "^12.12.17",
    "apexcharts": "^3.33.0",
    "apollo-cache-inmemory": "^1.6.3",
    "apollo-client": "2.6.8",
    "apollo-link": "^1.2.12",
    "apollo-link-context": "^1.0.20",
    "apollo-link-error": "^1.1.11",
    "apollo-upload-client": "^12.1.0",
    "axios": "^0.19.0",
    "cookie": "^0.4.0",
    "cross-env": "^6.0.3",
    "cross-fetch": "^3.0.6",
    "form-data": "^3.0.0",
    "graphql": "^14.0.2",
    "graphql-tag": "^2.10.1",
    "intersection-observer": "^0.11.0",
    "js-cookie": "^2.2.1",
    "jump.js": "^1.0.2",
    "laravel-echo": "^1.6.1",
    "lodash": "^4.17.11",
    "luxon": "^1.21.3",
    "nuxt": "^2.14.0",
    "nuxt-property-decorator": "^2.7.2",
    "pdfvuer": "^1.7.5",
    "pusher-js": "^5.1.1",
    "sweet-modal-vue": "^2.0.0",
    "tiptap": "^1.26.6",
    "tiptap-extensions": "^1.28.6",
    "ts-node": "^8.5.4",
    "v-calendar": "^2.2.3",
    "v-tooltip": "2.0.2",
    "vee-validate": "^3.2.0",
    "vue-advanced-cropper": "^0.16.5",
    "vue-apexcharts": "^1.6.2",
    "vue-apollo": "^3.0.0-rc.2",
    "vue-currency-filter": "^3.4.3",
    "vue-multiselect": "^2.1.3",
    "vue-router": "^3.0.7",
    "vue-slide-up-down": "^2.0.0",
    "vuex": "^3.1.1",
    "vuex-class": "^0.3.2"
  },
  "devDependencies": {
    "@babel/runtime-corejs2": "^7.8.4",
    "@graphql-codegen/cli": "^1.9.1",
    "@graphql-codegen/fragment-matcher": "^1.9.1",
    "@graphql-codegen/schema-ast": "^1.9.1",
    "@graphql-codegen/typescript": "^1.9.1",
    "@graphql-codegen/typescript-operations": "1.17.12",
    "@nuxt/types": "^2.14.0",
    "@nuxt/typescript-build": "^2.0.2",
    "@nuxtjs/tailwindcss": "^3.4.2",
    "@tailwindcss/postcss7-compat": "2.0.2",
    "@types/cookie": "^0.3.2",
    "@types/js-cookie": "^2.2.3",
    "@types/jump.js": "^1.0.2",
    "@types/lodash": "^4.14.119",
    "@types/luxon": "^1.21.0",
    "autoprefixer": "^10.4.7",
    "core-js": "2",
    "local-web-server": "^3.0.7",
    "node-sass": "^6.0.1",
    "postcss": "^8.4.14",
    "postcss-import": "12.0.1",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.0",
    "sass-loader": "^10.2.0",
    "tailwindcss": "^3.1.6",
    "tslint": "^5.20.1",
    "tslint-config-prettier": "^1.17.0",
    "tslint-plugin-prettier": "^2.0.1"
  },
  "resolutions": {
    "prosemirror-model": "1.8.2",
    "prosemirror-state": "1.3.2",
    "prosemirror-transform": "1.2.2",
    "prosemirror-view": "1.13.4"
  }
}
